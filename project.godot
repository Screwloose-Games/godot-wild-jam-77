; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="Godot Wild Jam 77"
run/main_scene="res://common/ui/main_menu/main_menu.tscn"
config/features=PackedStringArray("4.3")
boot_splash/image="res://splash_screen.png"
config/icon="res://icon.svg"

[autoload]

Components="*res://addons/Component/Components.gd"
SceneTransitionManager="*res://globals/scene_transition_manager.gd"
SceneManager="*res://globals/scene_manager.gd"
GameSettings="*res://globals/game_settings.tscn"
ControllerIcons="*res://addons/controller_icons/ControllerIcons.gd"
PhantomCameraManager="*res://addons/phantom_camera/scripts/managers/phantom_camera_manager.gd"

[editor_plugins]

enabled=PackedStringArray("res://addons/controller_icons/plugin.cfg", "res://addons/phantom_camera/plugin.cfg", "res://addons/screwloose-editor-settings/plugin.cfg")

[gui]

theme/custom="res://common/themes/global_theme.tres"

[input]

pause={
"deadzone": 0.5,
"events": [Object(InputEventKey,"resource_local_to_scene":false,"resource_name":"","device":-1,"window_id":0,"alt_pressed":false,"shift_pressed":false,"ctrl_pressed":false,"meta_pressed":false,"pressed":false,"keycode":0,"physical_keycode":80,"key_label":0,"unicode":112,"location":0,"echo":false,"script":null)
]
}

[rendering]

renderer/rendering_method="gl_compatibility"
renderer/rendering_method.mobile="gl_compatibility"
