config_version=5

[addons]

resources_spreadsheet_view/array_color_tint=100.0
resources_spreadsheet_view/color_rows=true
resources_spreadsheet_view/array_min_width=128.0
resources_spreadsheet_view/resource_preview_size=32.0
resources_spreadsheet_view/clip_headers=false
resources_spreadsheet_view/dupe_arrays=true
resources_spreadsheet_view/context_menu_on_leftclick=false
resources_spreadsheet_view/fold_docks=true
resources_spreadsheet_view/resource_cell_label_mode=1

[application]

config/name="Godot Wild Jam 77"
run/main_scene="res://common/ui/main_menu/main_menu.tscn"
config/features=PackedStringArray("4.3")
boot_splash/image="res://splash_screen.png"
config/icon="res://icon.svg"

[autoload]

Components="*res://addons/Component/Components.gd"
SceneTransitionManager="*res://globals/scene_transition_manager.gd"
SceneManager="*res://globals/scene_manager.gd"
GameSettings="*res://globals/game_settings.tscn"
ControllerIcons="*res://addons/controller_icons/ControllerIcons.gd"
PhantomCameraManager="*res://addons/phantom_camera/scripts/managers/phantom_camera_manager.gd"
CheckpointMgr="*res://game/checkpoint_mgr.gd"
SoundManager="*res://addons/sound_manager/sound_manager.gd"
GlobalSignalBus="*res://globals/global_signal_bus.gd"
MusicController="*res://globals/music/music_controller.tscn"

[display]

window/stretch/mode="canvas_items"

[editor_plugins]

enabled=PackedStringArray("res://addons/controller_icons/plugin.cfg", "res://addons/phantom_camera/plugin.cfg", "res://addons/screwloose-editor-settings/plugin.cfg", "res://addons/sound_manager/plugin.cfg", "res://addons/zylann.hterrain/plugin.cfg")

[global_group]

